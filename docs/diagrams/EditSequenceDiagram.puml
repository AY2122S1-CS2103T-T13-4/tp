@startuml
!include style.puml

box Logic LOGIC_COLOR_T1
participant LogicManager LOGIC_COLOR
participant AddressBookParser LOGIC_COLOR
participant EditCommandParser LOGIC_COLOR
participant "e:EditCommand" as EditCommand LOGIC_COLOR
participant "p:Person" as Person LOGIC_COLOR
participant CommandResult LOGIC_COLOR
end box

box Model MODEL_COLOR_T1
participant Model MODEL_COLOR
end box

-> LogicManager : execute("edit 1 n/Peter e/peter@email.com g/M age/24")
activate LogicManager

LogicManager -> AddressBookParser : parseCommand("edit 1 n/Peter e/peter@email.com g/M age/24")
activate AddressBookParser

create EditCommandParser
AddressBookParser -> EditCommandParser
activate EditCommandParser
return

AddressBookParser -> EditCommandParser : parse("n/Peter e/peter@email.com g/M age/24")
activate EditCommandParser

create EditCommand
EditCommandParser -> EditCommand
activate EditCommand

create Person
EditCommand -> Person : new Person("Peter", "peter@email.com", "M", "24")
activate Person

return p

return e

EditCommandParser --> AddressBookParser : e
deactivate EditCommandParser
'Hidden arrow to position the destroy marker below the end of the activation bar.
EditCommandParser -[hidden]-> AddressBookParser
destroy EditCommandParser


LogicManager -> EditCommand : execute()
activate EditCommand

EditCommand -> Model : setPerson(1, p)
activate Model
return

create CommandResult
EditCommand -> CommandResult
activate CommandResult
return

return result

[<--LogicManager
deactivate LogicManager
@enduml